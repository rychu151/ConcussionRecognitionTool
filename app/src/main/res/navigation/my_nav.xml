<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.kubab.concussion.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_redFlagFragment"
            app:destination="@id/redFlagFragment" />
    </fragment>
    <fragment
        android:id="@+id/redFlagFragment"
        android:name="com.kubab.concussion.ui.diag.steps.ViewPagerFragment"
        android:label="fragment_red_flag"
        tools:layout="@layout/fragment_view_pager" />
</navigation>